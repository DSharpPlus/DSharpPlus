<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <PackageId>DSharpPlus.Http.AspNetCore</PackageId>
    <Description>A package to easily use HTTP-based Discord interactions with DSharpPlus in an ASP.NET Core project.</Description>
    <PackageTags>$(PackageTags), interactions, slash-commands, http-interactions</PackageTags>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsPackable>true</IsPackable>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="DSharpPlus.Natives.Sodium" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\DSharpPlus\DSharpPlus.csproj" />
  </ItemGroup>
</Project>
